    "import sys\n",
    "input = sys.stdin.readline\n",
    "start, end = map(int, input().split())\n",
    "numbers = list(map(int, input().split()))\n",
    "dp = [0 for i in range(0, len(numbers))]\n",
    "dp[1] = numbers[0]\n",
    "for i in range(2, len(numbers)):\n",
    "    dp[i] = numbers[i - 1] + dp[i - 1]\n",
    "    print(dp[i])\n",
    "\n",
    "\n"
   ]
